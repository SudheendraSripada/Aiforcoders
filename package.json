{
  "name": "ai-for-coders",
  "version": "1.0.0",
  "description": "AI for Coders - Complete web application, API backend, and desktop application",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck",
    "test": "turbo run test",
    "clean": "turbo run clean",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "deploy": "turbo run build && vercel deploy --prod",
    "setup": "pnpm install && turbo run build",
    "dev:web": "turbo run dev --filter=@ai-for-coders/web",
    "dev:api": "turbo run dev --filter=@ai-for-coders/api",
    "build:web": "turbo run build --filter=@ai-for-coders/web",
    "build:api": "turbo run build --filter=@ai-for-coders/api"
  },
  "devDependencies": {
    "turbo": "^2.3.3",
    "prettier": "^3.4.2",
    "eslint": "^8.57.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "typescript": "^5.8.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "keywords": [
    "ai",
    "coders",
    "web",
    "api",
    "angular",
    "gemini",
    "vercel",
    "turbo"
  ],
  "author": "AI for Coders Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/SudheendraSripada/ai-for-coders-web"
  },
  "bugs": {
    "url": "https://github.com/SudheendraSripada/ai-for-coders-web/issues"
  },
  "homepage": "https://github.com/SudheendraSripada/ai-for-coders-web#readme"
}